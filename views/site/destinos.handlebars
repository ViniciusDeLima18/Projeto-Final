    <main>
     <article>
 
       <!-- 
         HERO
       -->
 
       <section class="hero" id="home">
         <div class="container">
 
           <div class="dreamst">
           <p >DREAMS TURISMO</p>
           </div>

 
 
         </div>
 
         <div class="btn-group">
           <a href="/usuario/login"><button class="btn btn-primary">Fazer Login</button></a>
 
           <button class="btn btn-secondary">Agende agora</button>
         </div>
 
       </section>

 
 
       <!-- 
         - PACOTES
       -->
 
       <section class="package" id="package">
        <div class="container">
  
          <p class="section-subtitle">PACOTES</p>
  
          <h2 class="h2 section-title">Pacotes Populares</h2>
  
          <p class="section-text">
  
          </p>
  
          <ul class="package-list">
  

          {{#each response.data}}
            <li>
              <div class="package-card">
  
                <figure class="card-banner">
                  <img src="{{fotoDestaque}}" alt="{{titulo}}"
                    loading="lazy">
                </figure>
  
                <div class="card-content">
  
                  <h3 class="h3 card-title">{{titulo}}</h3>
  
                  <p class="card-text">
                    {{descricaoDestaque}}
                  </p>
  
                  <ul class="card-meta-list">
  
                    <li class="card-meta-item">
                      <div class="meta-box">
                        <ion-icon name="time"></ion-icon>
  
                        <p class="text">{{periodo}}</p>
                      </div>
                    </li>

                    <li class="card-meta-item">
                      <div class="meta-box">
                        <ion-icon name="location"></ion-icon>
  
                        <p class="text">{{hotel}}</p>
                      </div>
                    </li>

                    <li class="card-meta-item">
                      <div class="meta-box">
                        <ion-icon name="location"></ion-icon>
  
                        <p class="text">{{transporte}}</p>
                      </div>
                    </li>
    
                    <li class="card-meta-item">
                      <div class="meta-box">
                        <ion-icon name="location"></ion-icon>
  
                        <p class="text">{{destino}}, {{pais}}</p>
                      </div>
                    </li>
  
                  </ul>
  
                </div>
  
                <div class="card-price">
  
                  <div class="wrapper">
  
                    <p class="reviews">({{reviews}} reviews)</p>
  
                    <div class="card-rating">
                      <ion-icon name="star"></ion-icon>
                      <ion-icon name="star"></ion-icon>
                      <ion-icon name="star"></ion-icon>
                      <ion-icon name="star"></ion-icon>
                      <ion-icon name="star"></ion-icon>
                    </div>
  
                  </div>
  
                  <p class="price">
                    R$ {{valor}}
                    <span>/ por pessoa</span>
                  </p>
  
                  <button  class="btn btn-secondary"><a href="/site/confirmar">Agende agora</a></button>
  
                </div>
  
              </div>
            </li>

          {{else}}
          <div class="m-5">
              <h5>Sem itens cadastrados.</h5>
          </div>
          {{/each}}
          </ul>
  
        </div>
      </section>

      
       <!-- 
         - POPULARES
       -->
 
       <section class="popular" id="destination">
         <div class="container">
 
           <p class="section-subtitle">Descubra</p>
 
           <h2 class="h2 section-title">Destinos</h2>
 
           <p class="section-text">
 
           </p>
 
           <ul class="popular-list">
          {{#each response.secundario}}
             <li>
               <div class="popular-card">
 
                 <figure class="card-img">
                   <img src="{{fotoLista}}" alt="{{titulo}}" loading="lazy">
                 </figure>
 
                 <div class="card-content">
 
                   <div class="card-rating">
                     <ion-icon name="star"></ion-icon>
                     <ion-icon name="star"></ion-icon>
                     <ion-icon name="star"></ion-icon>
                     <ion-icon name="star"></ion-icon>
                     <ion-icon name="star"></ion-icon>
                   </div>
 
                   <p class="card-subtitle">
                     <a href=""{{pais}}</a>
                   </p>
 
                   <h3 class="h3 card-title">
                     <a href="/site/confirmar">{{destino}}</a>
                   </h3>
 
                   <p class="card-text">
                    {{descricao}}
                   </p>
 
                 </div>
 
               </div>
             </li>
             {{else}}
            <div class="m-5">
                <h5>Sem itens cadastrados.</h5>
            </div>
            {{/each}}
              
           </ul>
 
           <button class="btn btn-primary"><a href="/"> VOLTAR AO INICIO</a></button>
 
         </div>
    </section>
 
 
 
       <!-- 
         CTA
       -->
 
       <section class="cta" id="contact">
         <div class="container">
 
           <div class="cta-content">
             <p class="section-subtitle"></p>
 
             <h2 class="h2 section-title">Pronto para a viagem de seus sonhos. Lembre-se de n√≥s!</h2>
 
           </div>
 
           <button class="btn btn-secondary">Contate-nos!</button>
 
         </div>
       </section>
 
     </article>
   </main>
 
